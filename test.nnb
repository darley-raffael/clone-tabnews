                "interface Person {\n  name: string\n  age: number\n  code: string\n  isValid: boolean\n\n}\n\n\nconst person: Person = {\n  name: \"Darle\",\n  age: 25,\n  code: \"123456\",\n  isValid: true\n}\n\n// verificar os campos do objeto que foram alterados\nfunction ChangedObject(person: Person)  {\nlet prevValue: Person | null = null\nconst changedObj: Partial<Person> = {}\n\nif (!prevValue){\n  prevValue = {...person}\n\n}\n\nconsole.log(prevValue)\n\nfor (const [key, value] of Object.entries(person)) {\n  if (prevValue[key] !== person[key]) {\n    \n    changedObj[key] = value\n  }\n\n\n}\n\nreturn changedObj\n\n\n}\n\nChangedObject(person)"
